    "@langchain/slack": "https://esm.sh/@langchain/slack@0.0.4?bundle-deps",
    "@langchain/openai": "https://esm.sh/@langchain/openai@0.0.28?bundle-deps",
    "@langchain/anthropic": "https://esm.sh/@langchain/anthropic@0.1.19?bundle-deps"
  },
  "compilerOptions": {
    "allowJs": true,
    "lib": ["deno.window"],
    "strict": true
